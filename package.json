{
  "name": "boulderbunker",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint --quiet **/src/**/*.tsx",
    "lint:fix": "eslint . --fix **/src/**/*.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/App.tsx"
    ],
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{ts,tsx}",
      "./src/App.tsx"
    ]
  },
  "dependencies": {
    "@types/react-native": "~0.70.6",
    "expo": "~47.0.12",
    "expo-status-bar": "~1.4.2",
    "husky": "^8.0.3",
    "react": "18.1.0",
    "react-native": "0.70.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-jest": "^29.4.3",
    "eslint": "^8.35.0",
    "jest": "^29.4.3",
    "typescript": "^4.9.5"
  },
  "private": true
}
